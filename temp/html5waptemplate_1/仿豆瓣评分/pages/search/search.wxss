.search-group{
    padding:20rpx 30rpx;
}
.search-group .search-item{
    padding:20rpx 0;
    border-bottom: 1px solid #e5e5d5;
    display: flex;
    justify-content: flex-start;
}
.search-item .left-search-item{
    width:90rpx;
    height:120rpx;
    margin-right:20rpx;
}
.left-search-item image{
    width:100%;
    height:100%;
}
.right-search-item{
    flex: 1;
}
.right-search-item .title{
    font-size:40rpx;
}
.right-search-item .rate-year{
    font-size: 24rpx;
    color:#999;
    margin-top:20rpx;
}